{
    "type": "project",
    "license": "MIT",
    "name": "ayso/certification_reporting_system",
    "description": "An application to publish AYSO certification volunteer certifications",
    "require": {
        "php": "^7.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "laminas/laminas-code": "3.*",
        "laminas/laminas-eventmanager": "3.3.*",
        "phpoffice/phpspreadsheet": "1.*",
        "sensio/framework-extra-bundle": "5.6.*",
        "symfony/asset": "5.2.*",
        "symfony/console": "5.2.*",
        "symfony/dotenv": "5.2.*",
        "symfony/expression-language": "5.2.*",
        "symfony/flex": "1.*",
        "symfony/form": "5.2.*",
        "symfony/framework-bundle": "5.2.*",
        "symfony/intl": "5.2.*",
        "symfony/monolog-bundle": "3.*",
        "symfony/orm-pack": "2.*",
        "symfony/process": "5.2.*",
        "symfony/security-bundle": "5.2.*",
        "symfony/serializer-pack": "^1",
        "symfony/twig-bundle": "5.2.*",
        "symfony/validator": "5.2.*",
        "symfony/web-link": "5.2.*",
        "symfony/webpack-encore-bundle": "1.*",
        "symfony/yaml": "5.2.*"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "6.5.*",
        "phpunit/phpunit": "9.5.*",
        "symfony/apache-pack": "1.*",
        "symfony/browser-kit": "5.2.*",
        "symfony/css-selector": "5.2.*",
        "symfony/debug-pack": "1.*",
        "symfony/http-client": "5.2.*",
        "symfony/maker-bundle": "1.*",
        "symfony/profiler-pack": "1.*",
        "symfony/test-pack": "1.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "platform": {
            "php": "7.3"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^5.2"
        }
    }
}
